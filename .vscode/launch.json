    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) test_wip",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceRoot}/bin/test_wip",
            "args": [],
            "preLaunchTask": "nimbuild - test_wip",
            "stopAtEntry": false,
            "cwd": "${workspaceRoot}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "nim-gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "logging": {
                "engineLogging": true
            },
        },
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceRoot}/bin/gene",
            "args": ["${workspaceRoot}/bin/render_html", "${workspaceRoot}/examples/html.gene"],
            "preLaunchTask": "nimbuild",
            "stopAtEntry": false,
            "cwd": "${workspaceRoot}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "nim-gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "logging": {
                "engineLogging": true
            },
        },
    ]
}