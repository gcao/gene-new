#!/usr/bin/env gene
(fn fib n
  (if (n < 2)
    n
  else
    ((fib (n - 1)) + (fib (n - 2)))
  )
)
(if (($cmd_args .size) > 1)
  (var i (($cmd_args .@1).to_i))
  (var start (gene/now))
  (println "fib(" i ") = " (fib i))
  (println "Execution time: " (start .elapsed) " seconds.")
else
  (println "Usage:\nfib.gene <n>")
)
